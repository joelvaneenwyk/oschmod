{
    "[python]": {
        "diffEditor.ignoreTrimWhitespace": false,
        "editor.codeActionsOnSave": {
            "source.fixAll": "explicit",
            "source.organizeImports": "explicit"
        },
        "editor.defaultFormatter": "charliermarsh.ruff",
        "editor.formatOnSave": true,
        "editor.formatOnSaveMode": "modificationsIfAvailable",
        "editor.formatOnType": true,
        "editor.wordBasedSuggestions": "allDocuments",
        "gitlens.codeLens.symbolScopes": ["!Module"]
    },
    "[toml]": {
        "editor.defaultFormatter": "tamasfe.even-better-toml"
    },
    "cSpell.words": [
        "aceflags",
        "Autobuild",
        "autorelease",
        "bdist",
        "bumpversion",
        "codeql",
        "distutils",
        "docstrings",
        "endlocal",
        "ERRORLEVEL",
        "inifile",
        "IRUSR",
        "IWUSR",
        "IXUSR",
        "Mergify",
        "newver",
        "ntsecuritycon",
        "oneline",
        "oschmod",
        "pipx",
        "pipywin",
        "pluggy",
        "pyenv",
        "pylint",
        "pypi",
        "pyproject",
        "PYTHONDONTWRITEBYTECODE",
        "PYWIN",
        "pywinerror",
        "pywintypes",
        "redef",
        "rootdir",
        "sdist",
        "setlocal",
        "structseq",
        "topdir",
        "typeshed",
        "ugoa",
        "USERPROFILE"
    ],
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.codeActionsOnSave": {
        "source.organizeImports": "explicit"
    },
    "editor.defaultFormatter": "charliermarsh.ruff",
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.wordBasedSuggestions": "allDocuments",
    "evenBetterToml.formatter.alignComments": true,
    "evenBetterToml.formatter.allowedBlankLines": 1,
    "evenBetterToml.formatter.arrayAutoCollapse": true,
    "evenBetterToml.formatter.arrayTrailingComma": false,
    "evenBetterToml.formatter.columnWidth": 120,
    "evenbettertoml.formatter.quoteStyle": "auto",
    "evenBetterToml.formatter.reorderArrays": false,
    "evenBetterToml.formatter.trailingNewline": true,
    "evenBetterToml.schema.links": true,
    "evenBetterToml.semanticTokens": true,
    "gitlens.codeLens.symbolScopes": ["!Module"],
    "mypy-type-checker.preferDaemon": true,
    "mypy-type-checker.reportingScope": "workspace",
    "mypy-type-checker.severity": {
        "error": "Error",
        "info": "Warning",
        "note": "Warning",
        "warning": "Warning"
    },
    "pylint.lintOnChange": true,
    "pylint.severity": {
        "convention": "Warning",
        "refactor": "Warning"
    },
    "pylint.showNotifications": "always",
    "python.analysis.addImport.exactMatchOnly": true,
    "python.analysis.autoFormatStrings": true,
    "python.analysis.completeFunctionParens": true,
    "python.analysis.diagnosticMode": "workspace",
    "python.analysis.enablePytestExtra": true,
    "python.analysis.enablePytestSupport": true,
    "python.analysis.enableSyncServer": false,
    "python.analysis.inlayHints.callArgumentNames": "all",
    "python.analysis.inlayHints.functionReturnTypes": true,
    "python.analysis.inlayHints.pytestParameters": true,
    "python.analysis.inlayHints.variableTypes": true,
    "python.analysis.logLevel": "Warning",
    "python.analysis.typeCheckingMode": "strict",
    "python.analysis.userFileIndexingLimit": 5000,
    "python.createEnvironment.contentButton": "show",
    "python.languageServer": "Pylance",
    "python.missingPackage.severity": "Error",
    "python.terminal.activateEnvInCurrentTerminal": true,
    "python.terminal.executeInFileDir": false,
    "python.terminal.focusAfterLaunch": true,
    "python.testing.pytestArgs": ["tests"],
    "python.testing.pytestEnabled": true,
    "python.testing.unittestEnabled": false,
    "pythonTestExplorer.outputs.collectOutputs": true,
    "pythonTestExplorer.outputs.showOutputsOnRun": true,
    "pythonTestExplorer.testFramework": "pytest",
    "pythonTestExplorer.testplanEnabled": true
}
